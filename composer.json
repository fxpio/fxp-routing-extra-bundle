{
    "name": "sonatra/routing-extra-bundle",
    "type": "symfony-bundle",
    "description": "A set of extra tools for the symfony routing",
    "keywords": ["routing", "extra", "bundle"],
    "homepage": "https://github.com/sonatra/sonatra-routing-extra-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/routing": "^3.4.0|^4.0.0",
        "symfony/property-access": "^3.4.0|^4.0.0",
        "sonatra/routing-extra": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\RoutingExtraBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
